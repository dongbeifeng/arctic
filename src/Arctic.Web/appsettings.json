{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "NHibernate": {
    "CheckTransaction": true
  },
  "EventBus": {
    "Events": [
      {
        "EventType": "Hello",
        "Handlers": [
          "Arctic.EventBus.HelloEventHandler, Arctic.EventBus"
        ]
      }
    ]
  }
}
